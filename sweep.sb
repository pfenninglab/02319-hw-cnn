#!/bin/bash
# wandb_on_slurm.sb: Launch `wandb` sweep controller and agents on slurm.
# Adapted from https://github.com/elyall/wandb_on_slurm
# Usage: sbatch sweep.sb

#SBATCH --nodes=1
#SBATCH -p pfen3
#SBATCH --gres gpu:1
#SBATCH -n 1
#SBATCH -o /home/%u/outputs/out.txt
#SBATCH -e /home/%u/outputs/err.txt
#SBATCH --exclude compute-4-20

# cuda module and corresponding conda env
module load cuda-10.2
source activate keras2-tf27

python wandb_on_slurm.py -sweep_config sweep-config.yaml
