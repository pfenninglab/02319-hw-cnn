#!/bin/bash

# all paths must be absolute

#SBATCH -p pfen3
#SBATCH --gres gpu:1
#SBATCH -n 1
#SBATCH -o /home/%u/outputs/out.txt
#SBATCH -e /home/%u/outputs/err.txt
#SBATCH --exclude compute-4-20

# cuda module and corresponding conda env
module load cuda-10.2
source activate keras2-tf27

python train.py -config $1 > /home/$(whoami)/outputs/result.txt
